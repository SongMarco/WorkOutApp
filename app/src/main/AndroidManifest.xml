<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nova.workoutapp22">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />

    <application
        android:name=".subSources.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="nova.workoutapp22.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nova.workoutapp22.MotivationActivity"
            android:label="동기부여 자료" />
        <activity
            android:name="nova.workoutapp22.WorkoutAphroActivity"
            android:label="아프로디테"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name="nova.workoutapp22.WorkoutActivity"
            android:label="운동 프로그램" />

        <activity
            android:name="nova.workoutapp22.WorkoutMemoActivity"
            android:label="운동 일지22">

        </activity>

        <activity
            android:name="nova.workoutapp22.AddMemoActivity"
            android:label="일기 추가하기">

        </activity>



        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="nova.workoutapp22.provider"
            android:exported="false"
            android:grantUriPermissions="true">
        <meta-data
            android:name="android.support.FILE_PROVIDER_PATHS"
            android:resource="@xml/provider_paths" />
        </provider>
    </application>

</manifest>